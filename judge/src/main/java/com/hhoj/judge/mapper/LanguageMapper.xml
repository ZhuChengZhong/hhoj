<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="LanguageMapper">
    <select id="findById" parameterType="Integer" resultType="Submit">
        select *from student where id=#{id}
    </select>
    <select id="count" resultType="Integer">
    	select count(*) from t_submit;
    </select>
    <insert id="insert" parameterType="Language" useGeneratedKeys="true" keyProperty="languageId">
    	insert into t_language values(NULL,#{languageName});
    </insert>
</mapper>